{
    "name": "appkit/bunny",
    "type": "library",
    "description": "Fork of the Bunny AMQP client, enhanced with fixes and improvements for the AppKit framework",
    "keywords": [
        "rabbitmq",
        "rabbit",
        "bunny",
        "amqp",
        "messaging",
        "message",
        "queue",
        "queueing",
        "exchange",
        "react",
        "react-php",
        "reactphp",
        "async",
        "await",
        "fibers"
    ],
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Jakub Kulhan",
            "email": "jakub.kulhan@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "evenement/evenement": "^3.0",
        "react/async": "^4.2",
        "react/event-loop": "^1.5",
        "react/promise": "^3.1",
        "react/socket": "^1.15"
    },
    "autoload": {
        "psr-4": {
            "Bunny\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
